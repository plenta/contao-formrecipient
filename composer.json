  "keywords":["contao", "form", "recipient"],
  "type":"contao-module",
  "license":"LGPL-3.0+",
  "authors":[
    {
      "name":"Christoph Barkowsky",
      "email":"hallo@christianbarkowsky.de",
      "homepage":"http://christianbarkowsky.de",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"hallo@christianbarkowsky.de",
    "issues":"https://github.com/scyfel/contao-formrecipient/issues",
    "source":"https://github.com/scyfel/contao-formrecipient"
